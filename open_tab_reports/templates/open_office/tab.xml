    <office:body>
        <office:text>
            <text:sequence-decls>
                <text:sequence-decl text:display-outline-level="0" text:name="Illustration"/>
                <text:sequence-decl text:display-outline-level="0" text:name="Table"/>
                <text:sequence-decl text:display-outline-level="0" text:name="Text"/>
                <text:sequence-decl text:display-outline-level="0" text:name="Drawing"/>
            </text:sequence-decls>
            <text:h text:outline-level="1" text:style-name="P1">{{tournament_name}} - Tab</text:h>
            <text:section text:name="Section1" text:style-name="Sect1">
                <text:p text:style-name="P2">Teams</text:p>
                <table:table table:name="Table2" table:style-name="Table2">
                    <table:table-column table:style-name="Table2.A"/>
                    <table:table-column table:style-name="Table2.B"/>
                    <table:table-column table:style-name="Table2.C"/>
                    <table:table-column table:style-name="Table2.D"/>
                    {% for entry in team_tab %}
                    <table:table-row style="Table2.1">
                        <table:table-cell office:value-type="string" table:style-name="Table2.A1">
                            <text:p text:style-name="P10">
                                <text:span text:style-name="T1">{{entry.rank + 1}}.</text:span></text:p>
                        </table:table-cell>
                        <table:table-cell office:value-type="string" table:style-name="Table2.B1">
                            <text:p text:style-name="P9">{{entry.team_name}}
                                <text:span text:style-name="T11">
                                    {% if break_marks is containing(entry.team_uuid) %}
                                        ({{ break_marks[entry.team_uuid]|join(sep=",") }})
                                    {% endif %}
                                </text:span>
                            </text:p>
                            <text:p text:style-name="P6">{{entry.member_ranks|sort|join(sep="+")}}</text:p>
                            <text:p text:style-name="P6">
                {% for detailed_score in entry.detailed_scores%}
                    {% if detailed_score %}
                        {% set team_score = detailed_score.team_score | unwrap_default(default=0.0) %}
                        
                        {% if not loop.first %}
                        +
                        {% endif %}
                        {{(detailed_score.speaker_score + team_score) | to_2_decimals}}
                    {% endif %}
                {% endfor %}
                            </text:p>
                        </table:table-cell>
                        <table:table-cell office:value-type="string" table:style-name="Table2.A1">
                            <text:p text:style-name="P14">
                                {{entry.total_score | to_2_decimals}}
                            </text:p>
                        </table:table-cell>
                        <table:table-cell office:value-type="string" table:style-name="Table2.A1">
                            <text:p text:style-name="P14">
                                {% if entry.avg_score %}
                                    {{entry.avg_score | to_2_decimals}}
                                {% endif %}
                            </text:p>
                        </table:table-cell>
                    </table:table-row>
                    {% endfor %}
                </table:table>
                <text:p text:style-name="P2"/>
                <text:p text:style-name="P2">Redner</text:p>
                <table:table table:name="Table1" table:style-name="Table1">
                    <table:table-column table:style-name="Table1.A"/>
                    <table:table-column table:style-name="Table1.B"/>
                    <table:table-column table:style-name="Table1.C"/>
                    <table:table-column table:style-name="Table1.D"/>
                    {% for entry in speaker_tab %}

                    <table:table-row table:style-name="Table2.1">
                        <table:table-cell office:value-type="string" table:style-name="Table1.A1">
                            <text:p text:style-name="P12">
                                <text:span text:style-name="T1">{{entry.rank + 1}}</text:span>.</text:p>
                        </table:table-cell>
                        <table:table-cell office:value-type="string" table:style-name="Table1.B1">
                            <text:p text:style-name="P13">{{entry.speaker_name|public_name}}
                                <text:span text:style-name="T11">
                                    {% if break_marks is containing(entry.speaker_uuid) %}
                                        ({{ break_marks[entry.speaker_uuid]|join(sep=",") }})
                                    {% endif %}
                                </text:span>
                            </text:p>
                            <text:p text:style-name="P6">{{entry.team_name}} ({{entry.detailed_scores | role_letters | join(sep="+")}})</text:p>
                            <text:p text:style-name="P7">
                                {% for detailed_score in entry.detailed_scores%}
                                    {% if detailed_score %}
                                        {% if not loop.first %}
                                        +
                                        {% endif %}
                                        {{detailed_score.score|to_2_decimals}}
                                    {% endif %}
                                {% endfor %}
                            </text:p>
                        </table:table-cell>
                        <table:table-cell office:value-type="string" table:style-name="Table2.A1">
                            <text:p text:style-name="P14">
                                {{entry.total_score | to_2_decimals}}
                            </text:p>
                        </table:table-cell>
                        <table:table-cell office:value-type="string" table:style-name="Table2.A1">
                            <text:p text:style-name="P14">
                                {% if entry.avg_score %}
                                    {{entry.avg_score | to_2_decimals}}
                                {% endif %}
                            </text:p>
                        </table:table-cell>
                    </table:table-row>
                    {% endfor %}
                </table:table>
                <text:p text:style-name="P3"/>

                {% if breaking_adjudicators %}
                <text:p text:style-name="P11">Jurybreak</text:p>
                <text:p text:style-name="P11">
                    <text:span text:style-name="T4"/>
                </text:p>
                <text:p text:style-name="P13">
                    {{breaking_adjudicators | map(attribute="name") | join(sep=", ")}}
                </text:p>
                {% endif %}
            </text:section>
        </office:text>
    </office:body>
